                                      onAction="#switchConfig" prefHeight="35.0" prefWidth="130.0" text="Configuracion"
                                      textFill="#757575" toggleGroup="$Home">
                            <graphic>
                                <ImageView fitHeight="18.0" fitWidth="18.0" opacity="0.51" pickOnBounds="true"
                                           preserveRatio="true">
                                    <image>
                                        <Image url="@../../imagen/icono/baseline_dashboard_black_18.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </ToggleButton>
                    </children>
