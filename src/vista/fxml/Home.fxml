<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="582.0"
            prefWidth="752.0" stylesheets="@../estilo/Style.css"
            xmlns="http://javafx.com/javafx/8.0.171" fx:controller="controlador.HomeController">
    <center>
        <ScrollPane fitToWidth="true" focusTraversable="false" hbarPolicy="NEVER" pickOnBounds="false" styleClass="root"
                    vbarPolicy="NEVER" BorderPane.alignment="CENTER">
            <content>
                <StackPane fx:id="holderPane" alignment="BASELINE_CENTER" styleClass="back"/>
            </content>
        </ScrollPane>
    </center>
    <bottom>
        <StackPane fx:id="footer" minHeight="-Infinity" minWidth="-Infinity" styleClass="backfoot"
                   BorderPane.alignment="CENTER">
            <children>
                <StackPane fx:id="footer"/>
            </children>
        </StackPane>
    </bottom>
    <left>
        <StackPane BorderPane.alignment="CENTER">
            <children>
                <VBox prefHeight="554.0" styleClass="vbox-blanco">
                    <children>
                        <ToggleButton id="sidebuttons" fx:id="buttonClientes" alignment="BASELINE_LEFT"
                                      onAction="#switchClientes" prefHeight="35.0" prefWidth="130.0" text="Clientes"
