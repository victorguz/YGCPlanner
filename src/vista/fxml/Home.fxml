                                <ImageView fitHeight="18.0" fitWidth="18.0" opacity="0.51" pickOnBounds="true"
                                           preserveRatio="true">
                                    <image>
                                        <Image url="@../../imagen/icono/baseline_fitness_center_black_18.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </ToggleButton>
                        <Separator prefHeight="10.0"/>
