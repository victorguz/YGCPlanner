                            <graphic>
                                <ImageView fitHeight="18.0" fitWidth="18.0" opacity="0.51" pickOnBounds="true"
                                           preserveRatio="true">
                                    <image>
                                        <Image url="@../../imagen/icono/baseline_face_black_18.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                            <toggleGroup>
                                <ToggleGroup fx:id="Home"/>
                            </toggleGroup>
                        </ToggleButton>
                        <ToggleButton id="sidebuttons" fx:id="buttonMedidas" alignment="BASELINE_LEFT"
                                      onAction="#switchMedidas" prefHeight="35.0" prefWidth="130.0" text="Medidas"
