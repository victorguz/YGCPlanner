                                <ImageView fitHeight="18.0" fitWidth="18.0" opacity="0.51" pickOnBounds="true"
                                           preserveRatio="true">
                                    <image>
                                        <Image url="@../../imagen/icono/baseline_assignment_black_18.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </ToggleButton>
                        <Separator prefHeight="10.0"/>
                        <ToggleButton id="sidebuttons" fx:id="buttonDietas" alignment="BASELINE_LEFT"
                                      onAction="#switchDietas" prefHeight="35.0" prefWidth="130.0" text="NutriciÃ³n"
                                      textFill="#757575" toggleGroup="$Home">
                            <graphic>
                                <ImageView fitHeight="18.0" fitWidth="18.0" opacity="0.51" pickOnBounds="true"
                                           preserveRatio="true">
                                    <image>
                                        <Image url="@../../imagen/icono/baseline_favorite_border_black_18.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </ToggleButton>
                        <ToggleButton id="sidebuttons" fx:id="buttonRutinas" alignment="BASELINE_LEFT"
                                      onAction="#switchRutinas" prefHeight="35.0" prefWidth="130.0" text="Entrenamiento"
                                      textFill="#757575" toggleGroup="$Home">
                            <graphic>
                                <ImageView fitHeight="18.0" fitWidth="18.0" opacity="0.51" pickOnBounds="true"
                                           preserveRatio="true">
                                    <image>
                                        <Image url="@../../imagen/icono/baseline_accessibility_new_black_18.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </ToggleButton>
                        <Separator prefHeight="10.0"/>
                        <ToggleButton id="sidebuttons" fx:id="buttonAlimentos" alignment="BASELINE_LEFT"
                                      onAction="#switchAlimentos" prefHeight="35.0" prefWidth="130.0" text="Alimentos"
                                      textFill="#757575" toggleGroup="$Home">
                            <graphic>
                                <ImageView fitHeight="18.0" fitWidth="18.0" opacity="0.51" pickOnBounds="true"
                                           preserveRatio="true">
                                    <image>
                                        <Image url="@../../imagen/icono/baseline_fastfood_black_18.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </ToggleButton>
                        <ToggleButton id="sidebuttons" fx:id="buttonEjercicios" alignment="BASELINE_LEFT"
                                      onAction="#switchEjercicios" prefHeight="35.0" prefWidth="130.0" text="Ejercicios"
                                      textFill="#757575" toggleGroup="$Home">
