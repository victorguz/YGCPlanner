<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane prefWidth="537.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.AlimentosController">
   <children>
      <VBox alignment="TOP_CENTER" styleClass="nada">
         <children>
            <VBox>
               <children>
                  <HBox alignment="TOP_CENTER" minHeight="-Infinity" minWidth="-Infinity" spacing="5.0">
                     <children>
                        <VBox styleClass="title">
                           <children>
                              <Label text="Buscar alimento" textFill="#b2b2b2">
                                    
                                 <padding>
                                    <Insets left="6.0" />
                                 </padding>
                              </Label>
                              <TextField fx:id="textBuscar" onAction="#buscar" onKeyTyped="#consumeDigits" prefHeight="40.0" prefWidth="130.0" promptText="Buscar alimento" />
                           </children>
                        </VBox>
                        <VBox styleClass="title">
                           <children>
                              <Label text="Seleccionar alimento" textFill="#b2b2b2">
                                    
                                 <padding>
                                    <Insets left="6.0" />
                                 </padding>
                              </Label>
                              <ComboBox fx:id="comboAlimentos" maxWidth="1.7976931348623157E308" minWidth="150.0" onAction="#mostrar" prefHeight="40.0" promptText="ALIMENTOS" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER">
               <children>
                  <Label text="Información del alimento (Porción de 100g)" textFill="#757575">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Label>
                  <FlowPane alignment="CENTER" prefHeight="111.0" styleClass="flowpane">
                     <children>
                        <VBox styleClass="title">
                           <children>
                              <Label text="Nombre" textFill="#b2b2b2">
                                    
                                 <padding>
                                    <Insets left="6.0" />
                                 </padding>
                              </Label>
                              <TextField fx:id="textNombre" prefHeight="40.0" prefWidth="130.0" promptText="Nombre" />
                           </children>
                        </VBox>
                        <VBox styleClass="title">
                           <children>
                              <Label text="Proteinas" textFill="#b2b2b2">
                                    
                                 <padding>
                                    <Insets left="6.0" />
                                 </padding>
                              </Label>
                              <TextField fx:id="textProteina" prefHeight="40.0" prefWidth="130.0" promptText="Proteinas" />
                           </children>
                        </VBox>
                        <VBox styleClass="title">
                           <children>
                              <Label text="Grasas" textFill="#b2b2b2">
                                    
                                 <padding>
                                    <Insets left="6.0" />
                                 </padding>
                              </Label>
                              <TextField fx:id="textGrasas" prefHeight="40.0" prefWidth="130.0" promptText="Grasas" />
                           </children>
                        </VBox>
                        <VBox styleClass="title">
                           <children>
                              <Label text="Carbohidratos" textFill="#b2b2b2">
                                    
                                 <padding>
                                    <Insets left="6.0" />
                                 </padding>
                              </Label>
                              <TextField fx:id="textCarbos" prefHeight="40.0" prefWidth="130.0" promptText="Carbohidratos" />
                           </children>
                        </VBox>
                        <VBox styleClass="title">
                           <children>
                              <Label text="Kilocalorias" textFill="#b2b2b2">
                                    
                                 <padding>
                                    <Insets left="6.0" />
                                 </padding>
                              </Label>
                              <TextField fx:id="textKilocalorias" editable="false" prefHeight="40.0" prefWidth="130.0" promptText="Kilocalorias" />
                           </children>
                        </VBox>
                     </children>
                  </FlowPane>
                  <HBox alignment="CENTER" spacing="10.0">
                     <children>
                        <JFXButton onAction="#eliminar" text="Eliminar" />
                        <JFXButton onAction="#limpiar" text="Limpiar" />
                        <JFXButton onAction="#modificar" text="Modificar" />
                        <JFXButton onAction="#registrar" text="Registrar" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER">
               <children>
                  <Label text="Resumen en porciones de 1g" textFill="#757575" />
                  <FlowPane alignment="CENTER" prefHeight="111.0" styleClass="flowpane">
                     <children>
                        <VBox styleClass="title">
                           <children>
                              <Label text="Kilocalorias" textFill="#b2b2b2">
                                    
                                 <padding>
                                    <Insets left="6.0" />
                                 </padding>
                              </Label>
                              <TextField fx:id="textKilocalorias1" editable="false" prefHeight="40.0" prefWidth="130.0" promptText="Kilocalorias" />
                           </children>
                        </VBox>
                        <VBox styleClass="title">
                           <children>
                              <Label text="Proteinas" textFill="#b2b2b2">
                                    
                                 <padding>
                                    <Insets left="6.0" />
                                 </padding>
                              </Label>
                              <TextField fx:id="textProteina1" editable="false" prefHeight="40.0" prefWidth="130.0" promptText="Proteinas" />
                           </children>
                        </VBox>
                        <VBox styleClass="title">
                           <children>
                              <Label text="Grasas" textFill="#b2b2b2">
                                    
                                 <padding>
                                    <Insets left="6.0" />
                                 </padding>
                              </Label>
                              <TextField fx:id="textGrasas1" editable="false" prefHeight="40.0" prefWidth="130.0" promptText="Grasas" />
                           </children>
                        </VBox>
                        <VBox styleClass="title">
                           <children>
                              <Label text="Carbohidratos" textFill="#b2b2b2">
                                    
                                 <padding>
                                    <Insets left="6.0" />
                                 </padding>
                              </Label>
                              <TextField fx:id="textCarbos1" editable="false" prefHeight="40.0" prefWidth="130.0" promptText="Carbohidratos" />
                           </children>
                        </VBox>
                     </children>
                  </FlowPane>
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</StackPane>
